{
  "name": "spidercalc",
  "version": "1.0.0",
  "description": "An educational game that teaches calculus, algebra, and probability through interactive Halloween-themed gameplay",
  "keywords": ["education", "calculus", "math", "game", "react", "phaser", "typescript"],
  "author": "Long Nguyen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:story": "VITE_DEFAULT_MODE=adventure VITE_DEFAULT_ADVENTURE_MODE=story vite",
    "typecheck": "tsc --noEmit",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "matter-js": "^0.20.0",
    "phaser": "^3.80.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/matter-js": "^0.20.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  }
}
